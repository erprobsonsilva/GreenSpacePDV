    DataSet = FdGeral
    Left = 254
    Top = 104
  end
  object FdGeral: TFDQuery
    Active = True
    AggregatesActive = True
    Connection = dm.ConnectionPostgres
    SQL.Strings = (
      'SELECT M.COD_ITEM_MOVIMENTO,'
      
        '       coalesce(P.COD_BARRA, CAST(P.COD_BALANCA as Character Var' +
        'ying(23))) CODIGO,'
      '       cast ((CASE'
      '       WHEN coalesce(LENGTH(P.NOME),0) = 0 THEN P.DESCRICAO'
      '       ELSE P.NOME'
      '       END) as Character Varying(23)) NOME,'
      'LENGTH(P.NOME),'
      '       M.QUANTIDADE,'
      '       M.PRECO_UNITARIO,'
      '       M.PRECO,'
      '       M.TP_UNIDADE_MEDIDA,'
      '       M.FLG_CANCELADO'
      '  FROM MOVIMENTO_ITEM M,'
      '       PRODUTO P'
      ' WHERE M.COD_PRODUTO = P.COD_PRODUTO'
      '   AND M.COD_MOVIMENTO = :COD_MOVIMENTO'
      'ORDER BY M.COD_ITEM_MOVIMENTO')
    Left = 254
    Top = 48
    ParamData = <
      item
        Name = 'COD_MOVIMENTO'
        DataType = ftInteger
        ParamType = ptInput
        Value = 71
      end>
